{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "quotes": [2, "double", { "avoidEscape": true }],
        "no-underscore-dangle":"off",
        "linebreak-style": ["error", "windows"],
        "indent": ["error", 4]

    }
    
}
